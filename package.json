{
  "name": "coons-patch",
  "version": "1.1.0",
  "packageManager": "pnpm@9.6.0",
  "description": "Create a Coons patch for a four-sided shape and manipulate the grid",
  "main": "dist/coons-patch.js",
  "exports": {
    ".": {
      "import": "./dist/coons-patch.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "/dist",
    "/src"
  ],
  "homepage": "https://coons-patch.undistraction.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undistraction/coons-patch.git"
  },
  "bugs": {
    "url": "https://github.com/undistraction/coons-patch/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "test-once": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./tests --watchAll",
    "lint": "pnpm run lint-prettier && pnpm run lint-eslint",
    "lint-prettier": "prettier . --write",
    "lint-eslint": "eslint './**/*.{js,jsx,cjs}'",
    "build": "vite build",
    "release": "semantic-release --no-ci --dry-run --debug"
  },
  "keywords": [
    "coons patch",
    "geometry",
    "math",
    "surface",
    "grid"
  ],
  "author": "Pedr Browne",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.6.0",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.1",
    "@types/node": "^20.14.9",
    "commitlint": "^19.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "3.3.2",
    "prop-types": "^15.8.1",
    "semantic-release": "^24.0.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "dependencies": {
    "fast-memoize": "^2.5.2",
    "matrix-js": "^1.7.1"
  },
  "pnpm": {
    "overrides": {
      "conventional-changelog-conventionalcommits": ">= 8.0.0"
    }
  }
}
